{
  "manifest_version": 2,

  "name": "Codechef Plugin",
  "description": "This extension demonstrates a codechef event.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  },

  "content_scripts": [    {
  		"matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["jquery.min.js","myjquery.js"]
    }
  ],

  "permissions": [
    "http://www.codechef.com/*"
  ]
}
